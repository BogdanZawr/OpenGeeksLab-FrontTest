{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-debugger": 0,
      "no-console": 0,
      "new-cap": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "eol-last": 0,
      "jsx-quotes": [1, "prefer-single"],
      "react/jsx-no-undef": 1,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "comma-spacing": [
        "error", {
            "before": false,
            "after": true
        }
    ],
    "no-multi-spaces": [
      "error", {
          "ignoreEOLComments": false
      }],
      "indent": [
        "error",
        4
    ],
    "strict": 0,
    "linebreak-style": [
        "error",
        "windows"
    ],
    "quotes": [
        "error",
        "single"
    ],
    "semi": [
        "error",
        "always"
    ],
    "lines-between-class-members": [
        "error", "always"
    ]
    }
  }